  <ItemGroup>
    <ProjectReference Include="..\AnointedAutomation.Logging\AnointedAutomation.Logging.csproj" />
    <ProjectReference Include="..\AnointedAutomation.Memory\AnointedAutomation.Memory.csproj" />

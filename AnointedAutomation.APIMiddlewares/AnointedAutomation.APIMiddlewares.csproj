<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/roku674/AnointedAutomation</PackageProjectUrl>
    <Title>AnointedAutomation.APIMiddlewares</Title>
    <Company>Anointed Automation LLC - Alexander Fields</Company>
    <Copyright>Copyright Â© 2023 Anointed Automation, LLC All rights reserved.</Copyright>
    <Description>Middlewares for APIs - AnointedAutomation.APIMiddlewares</Description>
    <RepositoryUrl>https://github.com/roku674/AnointedAutomation</RepositoryUrl>
    <Version>0.0.15</Version>
    <PackageTags>middleware</PackageTags>
    <PackageReleaseNotes>https://github.com/roku674/AnointedAutomation/pkgs/nuget/AnointedAutomation.APIMiddlewares</PackageReleaseNotes>
    <Authors>Anointed Automation LLC, Alexander Fields</Authors>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
